<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.31.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="1759610826203-1" author="AJ">
        <addColumn tableName="address">
            <column name="created_by" type="VARCHAR(255)"/>
            <column name="created_date" type="timestamp"/>
            <column name="last_modified_by" type="VARCHAR(255)"/>
            <column name="last_modified_date" type="timestamp"/>
            <column name="version" type="INT"/>
        </addColumn>
    </changeSet>
    <changeSet id="1759610826203-2" author="AJ">
        <addNotNullConstraint columnName="created_by" tableName="address"/>
    </changeSet>
    <changeSet id="1759610826203-4" author="AJ">
        <addNotNullConstraint columnName="created_date" tableName="address"/>
    </changeSet>

</databaseChangeLog>