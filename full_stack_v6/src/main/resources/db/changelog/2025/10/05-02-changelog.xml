<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.31.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="1759610826203-1" author="AJ">
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="address" columnName="created_by"/>
                <columnExists tableName="address" columnName="created_date"/>
                <columnExists tableName="address" columnName="last_modified_by"/>
                <columnExists tableName="address" columnName="last_modified_date"/>
                <columnExists tableName="address" columnName="version"/>
            </not>
        </preConditions>

        <addColumn tableName="address">
            <column name="created_by" type="VARCHAR(255) notnull"/>
            <column name="created_date" type="timestamptz notnull"/>
            <column name="last_modified_by" type="VARCHAR(255)"/>
            <column name="last_modified_date" type="timestamptz"/>
            <column name="version" type="INT"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>